<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--attributes sweet alert  -->
    <attr format="reference" name="sweet_alert_bg_drawable"/>
    <attr format="color|reference" name="sweet_alert_content_text_color"/>
    <attr format="color|reference" name="sweet_alert_title_text_color"/>
    <!--attar sweetRotate3DAnim -->
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum" >
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="fromDeg" format="float"/>
        <attr name="toDeg" format="float" />
        <attr name="customPivotX" format="fraction" />
        <attr name="customPivotY" format="fraction" />
    </declare-styleable>

    <!--attar ProgressWheel -->
    <declare-styleable name="ProgressWheel">
        <attr format="boolean" name="matProg_progressIndeterminate"/>
        <attr format="color" name="matProg_barColor"/>
        <attr format="color" name="matProg_rimColor"/>
        <attr format="dimension" name="matProg_rimWidth"/>
        <attr format="float" name="matProg_spinSpeed"/>
        <attr format="integer" name="matProg_barSpinCycleTime"/>
        <attr format="dimension" name="matProg_circleRadius"/>
        <attr format="boolean" name="matProg_fillRadius"/>
        <attr format="dimension" name="matProg_barWidth"/>
        <attr format="boolean" name="matProg_linearProgress"/>
    </declare-styleable>
    <!--attar circle Progress Button -->
    <declare-styleable name="CircularProgressButton">
        <attr format="reference" name="cpb_selectorIdle"/>
        <attr format="reference" name="cpb_selectorComplete"/>
        <attr format="reference" name="cpb_selectorError"/>
        <attr format="string" name="cpb_textComplete"/>
        <attr format="string" name="cpb_textIdle"/>
        <attr format="string" name="cpb_textError"/>
        <attr format="string" name="cpb_textProgress"/>
        <attr format="color" name="cpb_colorProgress"/>
        <attr format="color" name="cpb_colorIndicator"/>
        <attr format="color" name="cpb_colorIndicatorBackground"/>
        <attr format="reference" name="cpb_iconError"/>
        <attr format="reference" name="cpb_iconComplete"/>
        <attr format="dimension" name="cpb_cornerRadius"/>
        <attr format="dimension" name="cpb_paddingProgress"/>
    </declare-styleable>

</resources>